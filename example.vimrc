syntax on

set ts=4   " tab stops every 4 characters
set expandtab  " tabs to spaces
set nonu        " line nums
set noai        " autoindent off
set ai        " autoindent on
set nosi        " smartindent off
set si        " smartindent on
set cindent

filetype indent on

" replace "cca" with const char *
ab cca const char *    



